### Makefile for the FPU project
### Generated by Bluespec Workstation on Fri Jan 22 01:32:38 IST 2016

default: full_clean compile link simulate

.PHONY: compile
compile:
	@echo Compiling...
	bsc -u -sim -simdir . -bdir . -info-dir . -keep-fires -p %/Prelude:%/Libraries:%/Libraries/BlueNoC -g mkTb_fpu  fpu.bsv 
	@echo Compilation finished

.PHONY: compile_verilog
compile_verilog:
	@echo Compiling...
	bsc -u -verilog -elab -vdir . -bdir . -info-dir . -keep-fires -p %/Prelude:%/Libraries:%/Libraries/BlueNoC -g mkTb_fpu  fpu.bsv 
	@echo Compilation finished


.PHONY: link
link:
	@echo Linking...
	bsc -e mkTb_fpu -sim -o ./out -simdir . -p %/Prelude:%/Libraries:%/Libraries/BlueNoC -bdir . -keep-fires 
	@echo Linking finished


.PHONY: link_verilog
link_verilog:
	@echo Linking...
	bsc -e mkTb_fpu -verilog -o ./out -vdir .  -vsim cvc  -keep-fires ./mkTb_fpu.v
	@echo Linking finished



.PHONY: simulate
simulate:
	@echo Simulation...
	./out 
	@echo Simulation finished

